<starSchema name="shop">
  <aggregateFunc>MEAN</aggregateFunc>
  <aggregateFunc>SUM</aggregateFunc>
  <aggregateFunc>COUNT</aggregateFunc>
  <dimensionList>
    <dimension name="product">
      <attribute name="id"/>
      <attribute name="type"/>
    </dimension>
    <dimension name="item">
      <attribute name="code"/>
      <attribute name="name"/>
    </dimension>
  </dimensionList>
  <fact name="cost price" type="NUMERIC"/>
  <fact name="selling price" type="NUMERIC"/>
</starSchema>
