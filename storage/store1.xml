<starSchema name="store">
  <!--Optional:-->
  <dimensionList>
    <dimension name="customer">
      <attributes name="customer_id"/>
      <attributes name="name"/>
      <attributes name="age"/>
      <attributes name="sex"/>
    </dimension>
    <dimension name="item">
      <attributes name="item_id"/>
      <attributes name="category"/>
      <attributes name="subcategory"/>
    </dimension>
  </dimensionList>
  <factList>
    <fact name="sellingPrice">
      <aggregateFuncList>
        <aggregateFuncs>SUM</aggregateFuncs>
        <aggregateFuncs>AVG</aggregateFuncs>
        <aggregateFuncs>COUNT</aggregateFuncs>
      </aggregateFuncList>
      <type>NUMERIC</type>
    </fact>
    <fact name="profit">
      <aggregateFuncList>
        <aggregateFuncs>SUM</aggregateFuncs>
        <aggregateFuncs>AVG</aggregateFuncs>
      </aggregateFuncList>
      <type>NUMERIC</type>
    </fact>
  </factList>
</starSchema>


